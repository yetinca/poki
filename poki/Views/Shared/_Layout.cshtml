<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
  @Scripts.Render("~/bundles/jquery")
  @Scripts.Render("~/bundles/bootstrap")
  @Scripts.Render("~/bundles/modernizr")
  @RenderSection("scripts", required: false)
 
</head>
<body>
  <div class="container">
    <nav class="navbar navbar-default">
      <div class="container-fluid">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse-1" aria-expanded="false">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">Start</a>
        </div>
        <div class="collapse navbar-collapse" id="navbar-collapse-1">
          <ul class="nav navbar-nav">
            <li><a href="@Url.Action("GetGroups","Home")">Grupy <span class="sr-only">(current)</span></a></li>
            <li>@Html.ActionLink("Uczestnicy","Participants","Home")</li>
            <li class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Testy <span class="caret"></span></a>
              <ul class="dropdown-menu">
                <li><a href="#">Test Basic</a></li>
                <li><a href="#">Test 1</a></li>
                <li><a href="#">Test 2</a></li>
                <li role="separator" class="divider"></li>
                <li><a href="#">Raport 1</a></li>
                <li role="separator" class="divider"></li>
                <li><a href="#">Raport 2</a></li>
              </ul>
            </li>
          </ul>
          </div>
        </div>
    </nav>
  </div>




    
    <div class="container body-content">
        @RenderBody()
   
      <div class="navbar navbar-default navbar-fixed-bottom">
        <footer class="panel-footer ">
          <div class="container">
            <p class="text-muted">by Yetinca</p>
          </div>
        </footer>
      </div>
    </div>

  
</body>
</html>
